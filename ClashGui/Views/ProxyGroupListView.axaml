<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:designTime="clr-namespace:ClashGui.DesignTime"
             xmlns:views="clr-namespace:ClashGui.Views"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="ClashGui.Views.ProxyGroupListView">
    <Design.DataContext>
        <designTime:DesignProxyGroupListViewModel />
    </Design.DataContext>
    <ItemsControl Items="{Binding ProxyGroupViewModels}">
        <ItemsControl.ItemTemplate>
            <DataTemplate>
                <views:ProxyGroupView DataContext="{Binding}" />
            </DataTemplate>
        </ItemsControl.ItemTemplate>
    </ItemsControl>
</UserControl>