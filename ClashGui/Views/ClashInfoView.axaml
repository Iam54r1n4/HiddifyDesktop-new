<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:designTime="clr-namespace:ClashGui.DesignTime"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="ClashGui.Views.ClashInfoView">
    <Design.DataContext>
        <designTime:DesignClashInfoViewModel />
    </Design.DataContext>
    <StackPanel>
        <TextBlock Text="{Binding RealtimeSpeed}"></TextBlock>
        <TextBlock TextAlignment="Center" Text="Clash Version"></TextBlock>
        <TextBlock TextAlignment="Center" Text="{Binding Version}"></TextBlock>
    </StackPanel>
</UserControl>
