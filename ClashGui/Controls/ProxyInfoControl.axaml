<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:ClashGui.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="ClashGui.Controls.ProxyInfoControl">
    <Design.DataContext>
        <viewModels:ProxyInfoViewModel />
    </Design.DataContext>
    <StackPanel Orientation="Horizontal">
        <TextBlock Text="{Binding ProxyGroup.Name}"></TextBlock>
        <TextBlock Text="{Binding LatestHistory.Delay}"></TextBlock>
    </StackPanel>
</UserControl>